     * <span style="color: {{ first }};">@return</span> bool
     */</span>
    <span style="color: {{ second }};">public</span> <span style="color: {{ first }};">function</span> <span style="color: {{ third }};">isShared</span>($abstract)
    {
        <span style="color: {{ first }};">if</span> (<span style="color: {{ second }};">isset</span>($this<span style="color: {{ first }};">-></span>bindings[$abstract][<span style="color: {{ string }};">'shared'</span>]))
        {
            $shared <span style="color: {{ first }};">=</span> $this<span style="color: {{ first }};">-></span>bindings[$abstract][<span style="color: {{ string }};">'shared'</span>];
        }
        <span style="color: {{ first }};">else</span>
        {
            $shared <span style="color: {{ first }};">=</span> <span style="color: {{ second }};">false</span>;
        }

        <span style="color: {{ first }};">return</span> <span style="color: {{ second }};">isset</span>($this<span style="color: {{ first }};">-></span>instances[$abstract]) <span style="color: {{ first }};">||</span> $shared <span style="color: {{ first }};">===</span> <span style="color: {{ second }};">true</span>;
    }

}
